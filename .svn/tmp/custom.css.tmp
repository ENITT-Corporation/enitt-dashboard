.board_pager a.pageNow {background-color:#28A2FF; color:#FFFFFF; font-weight:bold}	
.board_pager a:hover {background-color:#28A2FF; color:#FFFFFF; font-weight:bold}	
.board_pager a img{vertical-align: middle;}




/*popup style*************************************************************************************************************************************************************************************************************************/
.pop_overlay { position:fixed; z-index:500; top:0; left:0; height:100%; width:100%; background-color:rgba(49, 61, 70, 0.8);}
.pop_layer { min-width:300px; z-index:600; background-color:#1B334A; position: absolute; left: 50%; top: 50%; transform: translate(-50%,-50%);}
.pop_layer .header { width:100%; height:48.5px; border-bottom: 1px solid #718090;}
.pop_layer .header .tit {float:left; font-weight: bold; font-size:18px; color: #FFFFFF; padding-left:19px; line-height:48.5px;}
.pop_layer .header .close { float:right; font-weight: bold; font-size:18px; color: #FFFFFF; width:27px; height:40px; line-height:48.5px; cursor:pointer;}
.pop_layer .con { clear:both; width:100%;}
.pop_layer .close .txt { float:right; color:#fff; font-size:13px; padding: 7px 10px 10px 5px;}
.pop_layer .close input { float:right; margin-top:8px;}
.pop_layer .pop_con_wrap {width: 100%;}
.pop_layer .pop_con_wrap .pop_con {padding: 13.5px;}

.statsTable{height: 500px; overflow: auto; width: 100%;}
.scatterPlotTable{height: 240px; overflow: auto;}
.table_st2 {width:100%;}
.table_st2 table thead tr th p.title{margin: 0px; font-size: 18px; font-weight: bold; padding-bottom: 10.5px;}
.table_st2 table tbody tr td .chartDiv{border: 1px solid #4F6070; max-height: 400px;margin:0px;}
.table_st2 table tbody tr td table{margin-top: 11px;}


/*tab style*************************************************************************************************************************************************************************************************************************/
.stab_dth { position:relative; width:100%; overflow:hidden; background-color: #0D253A; padding-left: 20px;}
.stab_dth ul {padding: 0px;}
.stab_dth ul li {height: 32px; line-height: 32px; float:left; padding: 4px 20px 4px 20px; cursor: pointer; background-color: #293948;  border: #293948; border-radius: 9px 9px 0px 0px; }
.stab_dth ul li:hover, .stab_dth li.selected  {background-color: #445261; border: #445261;}
.stab_dth ul li span{font-size:15px; color: #B9B9B9; font-weight: normal;}
.stab_dth ul li:hover span, .stab_dth li.selected span{color: #FFFFFF;}
.stab_dth ul li span.dts{background: url(/resources/css/lib/images/icon_temp_off.png) 0% 0% no-repeat; padding-left: 17px; background-position-y: center;}
.stab_dth ul li:hover span.dts, .stab_dth li.selected span.dts{background: url(/resources/css/lib/images/icon_temp_on.png) 0% 0% no-repeat; padding-left: 17px; background-position-y: center;}
.stab_dth ul li span.das{background: url(/resources/css/lib/images/icon_vibration_off.png) 0% 0% no-repeat; padding-left: 32px; background-position-y: center;}
.stab_dth ul li:hover span.das, .stab_dth li.selected span.das{background: url(/resources/css/lib/images/icon_vibration_on.png) 0% 0% no-repeat; padding-left: 32px; background-position-y: center;}


