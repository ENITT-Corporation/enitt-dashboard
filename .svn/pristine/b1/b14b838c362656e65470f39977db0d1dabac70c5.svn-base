.twinkle {animation-name:twinkle; animation-duration:1s; animation-iteration-count:infinite}

@-webkit-keyframes twinkle  { 0% { opacity: 0; }
  100% { opacity:1} }

@keyframes twinkle  { 0% { opacity: 0; }
  100% { opacity:1} }

